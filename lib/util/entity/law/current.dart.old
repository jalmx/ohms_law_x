import 'dart:math';

class Current {
  Current();

  static num calculateCurrent({required num voltage, required num resistance}) {
    return voltage / resistance;
  }

  static num calculateCurrentPowerResistance(
      {required num power, required num resistance}) {
    return sqrt(power * resistance);
  }

  static num calculateCurrentPowerVoltage(
      {required num power, required num voltage}) {
    return power / voltage;
  }
}
